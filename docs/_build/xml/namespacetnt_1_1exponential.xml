        <type>auto</type>
        <definition>auto tnt::exponential::EaseInOut</definition>
        <argsstring></argsstring>
        <name>EaseInOut</name>
        <initializer>= [](float time, float begin, float change, float duration) -&gt; float {
            if (time == 0)
                return begin;
            if (fabsf(time - duration) &lt; 1e-11)
                return change;
            if ((time /= duration / 2) &lt; 1)
                return change / 2 * powf(2, 10 * (time - 1)) + begin;
            return change / 2 * (-powf(2, -10 * (--time)) + 2) + begin;
        }</initializer>
